{
  "manifest_version": 3,
  "name": "Grimoire of Skillcraft",
  "version": "1.2",
  "description": "The Partyhat Clan's Scroll of Knowledge and Skill",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  },
  "permissions": [
    "sidePanel",
    "activeTab"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "side_panel": {
    "default_path": "popup.html"
  },
  "web_accessible_resources": [
    {
      "resources": ["parchment.jpg", "MedievalSharp.ttf"],
      "matches": ["<all_urls>"]
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}